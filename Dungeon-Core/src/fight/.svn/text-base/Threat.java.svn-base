package fight;
import figure.monster.Monster;

/**
 * Eine Drohung/Einschuechterung.
 *
 */
public class Threat{

	
	Monster m;


	int count;


    public Threat(Monster m){
	this.m = m;
	count = 1;
    }

	
	public Monster getMonster() {
		return m;
	}


    public void inc(){
	count++;
    }

	
	public int getCount() {
		return count;
	}

}
