
package dungeon;
import java.util.*;

/**
 * Vergleicht 2 Räume bzgl. der Anzahl ihrer Tueren
 *
 */
public class DoorCountRoomComparator implements Comparator{
	public int compare(Object o1, Object o2) {
				Room r1 = ((Room)o1);
				int value1 = (r1.getDoorCount());
				Room r2 = ((Room)o2);
				int value2 = (r2.getDoorCount());
				if(value1 < value2) {
					return 1;
				}
				else {
					return -1;	
				}
			
		
			}
			

}
